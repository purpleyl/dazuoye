<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="zjitc.zrt.blogspb.dao.ContentMapper">

    <select id="findAll" resultType="zjitc.zrt.blogspb.domain.Content">
        select * from t_content
    </select>
    <select id="findbyid" resultType="zjitc.zrt.blogspb.domain.Content">
        select  * from t_content where cid=#{id}
    </select>
</mapper>
